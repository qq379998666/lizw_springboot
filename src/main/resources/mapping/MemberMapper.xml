<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="lizw.springboot.mapper.MemberMapper" >
    <!-- 登录接口的sql语句 -->
    <select id="memberLogin" parameterType="lizw.springboot.entity.MemberEntity" resultType="java.util.Map">
        select * from member m  where m.phone = #{phone}
    </select>
</mapper>